{
  "name": "ai-model-registry-backend",
  "version": "1.0.0",
  "description": "Node.js backend services for AI Model Registry",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts --fix",
    "clean": "rm -rf dist node_modules",
    "migrate": "tsx src/scripts/migrate.ts",
    "db:status": "tsx src/scripts/migrate.ts status",
    "db:reset": "tsx src/scripts/migrate.ts reset",
    "seed": "tsx src/scripts/seed.ts",
    "seed:dev": "tsx src/scripts/seed.ts development"
  },
  "dependencies": {
    "@node-saml/passport-saml": "^5.1.0",
    "@types/express-session": "^1.18.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "aws-sdk": "^2.1445.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-openidconnect": "^0.1.2",
    "pg": "^8.11.3",
    "redis": "^5.8.3",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.8",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "supertest": "^6.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2",
    "vitest": "^0.34.0"
  }
}
